opal-tree-select/select (
	view-type={params.viewType},
	data-tree-list-keypath={_dataListKeypathParam},
	data-tree-list-item-schema={params.dataListItemSchema |json },
	value={params.value},
	view-model={viewModel},
	view-model-item-schema={params.viewModelItemSchema |json },
	add-new-item={params.addNewItem},
	focused={params.focused}
) {
	opal-sign-button/button (
		class=opal-select__button,
		sign=plus,
		checkable,
		tab-index={params.tabIndex},
		disabled={params.disabled}
	)
}