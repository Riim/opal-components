@section/inner {

div/list {
	@repeat (for={props.itemAs} of list) {
		rt-content/list-item (clone, get-context=_getListItemContext)
	}
}

opal-loader/loader (shown={loaderShown}, align-center={empty})

div/not-found (shown={notFoundShown}) {
	span/not-found-message { '{constructor.i18n.notFound}' }
}

}