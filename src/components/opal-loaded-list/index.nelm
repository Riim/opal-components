@section/inner {

div/list {
	@repeat (for={input.itemAs} of list) {
		rt-content/list-item (clone, get-context=_getListItemContext)
	}
}

opal-loader/loader (shown={loaderShown}, align-center={empty})

div/nothing-found (shown={nothingFoundShown}) {
	span/nothing-found-message { '{constructor.i18n.nothingFound}' }
}

}