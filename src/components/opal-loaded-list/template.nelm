@section/inner {

div/list {
	@repeat (for=$item of dataList) {
		rt-content/list-item (clone, get-context={_getListItemContext}) {
			'{$item |key(_dataListItemTextFieldName) }'
		}
	}
}

opal-loader/loader (shown={isLoaderShown}, align-center={empty})

@if-then (if=isNothingFoundShown) {
	div/nothing-found {
		span/nothing-found-message {
			'{constructor.i18n.nothingFound}'
		}
	}
}

}