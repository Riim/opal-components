@section/inner {

div/list {
	@repeat (for=$item of dataList) {
		rt-slot/list-item (clone-content, get-context=_getListItemContext) {
			'{$item |key(_dataListItemTextFieldName) }'
		}
	}
}

opal-loader/loader (shown={isLoaderShown}, align-center={empty})

div/nothing-found (shown={isNothingFoundShown}) {
	span/nothing-found-message { '{constructor.i18n.nothingFound}' }
}

}