@section/inner {

rt-slot/preset-rows-slot (
	for=preset-rows,
	not-have-new-rows={_notHaveNewRows},
	not-single-row={_notSingleRow}
) {
	rt-slot/preset-row-slot (for=preset-row)
}

div/new-rows (not-single-row={_notSingleRow}) {
	@repeat (for=row of _newRows, track-by=key) {
		rt-content/new-row-container (select='opal-multirow-row', clone, data-key={row.key})
	}
}

}