@section/inner {

rt-content/preset-rows-container (
	select='opal-multirow-row[preset], .opal-multirow__preset-rows',
	not-have-new-rows={_notHaveNewRows},
	not-single-row={_notSingleRow}
)

div/new-rows (not-single-row={_notSingleRow}) {
	@repeat (for=row of _newRows, track-by=key) {
		rt-content/new-row-container (select='opal-multirow-row:not([preset])', clone, data-key={row.key})
	}
}

}