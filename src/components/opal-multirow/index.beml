rt-content/preset-rows-container (
	select='opal-multirow-row[preset], .opal-multirow__preset-rows',
	not-have-new-rows={_notHaveNewRows},
	not-single-row={_notSingleRow}
)

div/new-rows (not-have-preset-rows={_notHavePresetRows}, not-single-row={_notSingleRow}) {
	template (is=rt-repeat, for=row of _newRows, track-by=key, rt-silent) {
		rt-content/new-row-container (select='opal-multirow-row:not([preset])', data-key={row.key})
	}
}