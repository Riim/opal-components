@section/inner {

RtSlot/presetRowsSlot (
	for=presetRows,
	not-have-new-rows={_notHaveNewRows},
	not-single-row={_notSingleRow}
) {
	RtSlot/presetRows (for=presetRow)
}

div/newRows (not-single-row={_notSingleRow}) {
	@Repeat (for=row of _newRows, track-by=key) {
		RtSlot/newRowSlot (clone-content, data-key={row.key})
	}
}

}