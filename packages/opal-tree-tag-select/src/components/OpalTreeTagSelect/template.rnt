OpalTreeSelect:select (
	viewType=clean,
	dataTreeListKeypath={_selectDataListKeypath},
	dataTreeListItemSchema={dataListItemSchema |dump },
	value={paramValue},
	viewModel={viewModel},
	viewModelItemSchema={viewModelItemSchema |dump },
	addNewItem={addNewItem},
	focused={focused}
) {
	RnSlot (class=OpalSelect__menuHeaderSlot, for=OpalSelect__menuHeader)

	OpalSignButton:button (
		class=OpalSelect__button,
		sign=plus,
		checkable,
		tabIndex={tabIndex},
		disabled={disabled}
	)
}